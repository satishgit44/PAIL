section .data
    firstName db 'Suraj', 10
    lenFirst equ $ - firstName

    lastName db 'Saw', 10
    lenLast equ $ - lastName

section .text
    global _start

_start:
    mov eax, 4
    mov ebx, 1
    mov ecx, firstName
    mov edx, lenFirst
    int 0x80

    mov eax, 4
    mov ebx, 1
    mov ecx, lastName
    mov edx, lenLast
    int 0x80

    mov eax, 1
    mov ebx, 0
    int 0x80
